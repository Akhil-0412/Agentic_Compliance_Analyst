{
    "framework": "nextjs",
    "functions": {
        "api/index.py": {
            "runtime": "python3.11",
            "maxDuration": 60
        }
    },
    "plugin": {
        "install": [
            "pip install -r requirements.txt"
        ]
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        }
    ]
}